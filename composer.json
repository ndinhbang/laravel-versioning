{
    "name": "neondigital/laravel-versioning",
    "description": "Versioning for Eloquent models",
    "type": "library",
    "require": {
      "laravel/framework": "^7.0|^8.0"
    },
    "require-dev": {
      "phpunit/phpunit": "^8.4@dev",
      "orchestra/testbench": "^5.0@dev"
    },
    "license": "MIT",
    "authors": [
      {
        "name": "Alec Ritson",
        "email": "alec@neondigital.co.uk"
      }
    ],
    "minimum-stability": "dev",
    "autoload": {
      "psr-4": {
        "NeonDigital\\Versioning\\": "src/"
      }
    },
    "autoload-dev": {
      "psr-4": {
        "Tests\\": "tests/"
      }
    },
    "extra": {
        "laravel": {
            "providers" : [
              "NeonDigital\\Versioning\\VersioningServiceProvider"
            ],
            "aliases": {
              "Versioning": "NeonDigital\\Versioning\\Facade"
            }
        }
    }
  }